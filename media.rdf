<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/storyMedia/"
     xml:base="http://www.semanticweb.org/ontologies/storyMedia/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:storyMedia="http://www.semanticweb.org/ontologies/storyMedia#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/storyMedia"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#featuresSong -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#featuresSong">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A song featured in a multimedial work. It doesn&apos;t matter if it was composed/recorded specifically for that work or not.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasCover -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasCover">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>Cover version of a song performed by another music artist.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasDirectSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasDirectSequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
        <rdfs:comment>A sequel that majorly features or references characters and plot points from its precedessing work. The sequel is harder to consume as a stand-alone work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasLooseSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasLooseSequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
        <rdfs:comment>A sequel that barely features or references characters and plot points from its precedessing work. It should be easy to consume as a stand-alone work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasNewRecording -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasNewRecording">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>Re-recording of a song by the same music artist.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#startsWithWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#startsWithWork">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Universe"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/belongsToFranchise -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/belongsToFranchise">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Franchise"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/belongsToSeries -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/belongsToSeries">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Series"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasContinuation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasContinuation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
        <rdfs:comment>A sequel that continues the same story, i.e. a single story that was split into multiple works.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasDirectAdaptation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasDirectAdaptation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasInspiredWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasInspiredWork">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasLooseAdaptation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasLooseAdaptation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasPrequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasPrequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasReboot -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasReboot">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasRemake -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasRemake">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSideStory -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSideStory">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSpinOff -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSpinOff">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/takesPlaceInUniverse -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/takesPlaceInUniverse">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Universe"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#Album -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#Album">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#MusicWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#MusicVideo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#MusicVideo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#MusicWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#MusicWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#Poem -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#Poem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#Song -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#Song">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#MusicWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#StagePlay -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#StagePlay">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#TheatricalProduction"/>
        <rdfs:comment>Text of a theatre play.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#TheatricalProduction -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#TheatricalProduction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#classicalSong -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#classicalSong">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A classical musical composition that can be identified by its composer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#musicalSong -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#musicalSong">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A musical composition that can be identified by a musical/opera/etc.. it was originally written for.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#recordedSong -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#recordedSong">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A concrete recording of a song that can be identified by its interpret and physical/digital release as a single or as a part of an album.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/CinematicWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/CompoundWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A grouping of multiple works that are somehow tied together into a bigger whole, usually by the same branding, telling the same story, using the same characters... The compound work itself is only an abstract concept, audience cannot consume it, only its individual parts.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Episode -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Episode">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Film -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Film">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Franchise -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Franchise">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/GameWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/GameWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/GraphicNovel -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/GraphicNovel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/LiteraryScreenplay -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/LiteraryScreenplay">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Novel -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Novel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Season -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Season">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Series -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Series">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/ShortStory -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/ShortStory">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Show -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Show">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/SingleWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/SingleWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A work that was produced as a single a standalone product.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/StoryCollection -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/StoryCollection">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/TabletopGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/TabletopGame">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/GameWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/VideoGame"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Universe -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Universe">
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A setting in which fictional stories can take place. It may be a wholy fictional universe/world/timeline, or simply a fictional depicture of the real world with some distinct features common among its works.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/VideoGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/VideoGame">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/GameWork"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Work -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Work">
        <rdfs:comment>A piece of art or media created by humans for artistic and/or entertainment purposes.</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#AnUnexpectedJourney -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/storyMedia#AnUnexpectedJourney">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Film"/>
        <belongsToSeries rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#HobbitMovieSeries"/>
        <hasContinuation rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#DesolationOfSmaug"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#BattleOfTheFiveArmies -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/storyMedia#BattleOfTheFiveArmies">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Film"/>
        <belongsToSeries rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#HobbitMovieSeries"/>
    </owl:NamedIndividual>
    


