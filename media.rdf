        <belongsToSeries rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#HobbitMovies"/>
        <hasContinuation rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#DesolationOfSmaug"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#AvatarTheLastAirbender -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ontologies/storyMedia#AvatarTheLastAirbender">
        <rdf:type rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Show"/>
        <storyMedia:hasFirstPart rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Book1Water"/>
    </owl:NamedIndividual>
    


