    <!-- http://www.semanticweb.org/ontologies/storyMedia#Album -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#Album">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#MusicWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A music album</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#MusicVideo -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#MusicVideo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
        <rdfs:comment>A video based on a song.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#MusicWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#MusicWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A medial work in the field of music.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#MusicalSong -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#MusicalSong">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A musical composition that can be identified by a musical/opera/etc.. it was originally written for.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#StagePlay"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#Poem -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#Poem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
        <rdfs:comment>A poem.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#RecordedSong -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#RecordedSong">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A concrete recording of a song that can be identified by its interpret and physical/digital release as a single or as a part of an album.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#Song -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#Song">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#MusicWork"/>
        <rdfs:comment>A song.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#StagePlay -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#StagePlay">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#StageProduction"/>
        <rdfs:comment>Text of a theatre play.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#StageProduction -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#StageProduction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork"/>
        <rdfs:comment>A concrete adaptation of a stage play by a specific group of people. Can be identified by the theatre group that created and performed it and the year it premiered.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#StagePlay"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia#TheatricalWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A medial work in the field of theatre.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/CinematicWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A medial work in the field of cinematography.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/CompoundWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A grouping of multiple works that are somehow tied together into a bigger whole. The compound work itself is only an abstract concept, audience cannot consume it, only its individual parts.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Episode -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Episode">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
        <rdfs:comment>Episode of a show.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Film -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Film">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
        <rdfs:comment>A film.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Franchise -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Franchise">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Series"/>
        <rdfs:comment>A wide group of works, usually across multiple types of media, related to some brand or intellectual property.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/GameWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/GameWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A medial work in the field of games.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/GraphicNovel -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/GraphicNovel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
        <rdfs:comment>A literary form that uses mostly pictures, e.g. a comics or a manga.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/LiteraryScreenplay -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/LiteraryScreenplay">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
        <rdfs:comment>An independently released literary screenplay, regardless of if it was made into a cinematic work.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
        <rdfs:comment>A medial work in the field of literature.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Novel -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Novel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
        <rdfs:comment>A book containing a single story.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Season -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Season">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
        <rdfs:comment>Season of a show.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Series -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Series">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
        <rdfs:comment>A sequence of closely related works, often sharing one common name and, in the case of story-based works, usually telling one story or more closely related stories.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/ShortStory -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/ShortStory">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
        <rdfs:comment>A shorter prose.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Show -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Show">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CinematicWork"/>
        <rdfs:comment>A serialised TV or streaming show.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/SingleWork -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/SingleWork">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A work that was produced as a single standalone product.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/StoryCollection -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/StoryCollection">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
        <rdfs:comment>A book containing multiple standalone short stories.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/TabletopGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/TabletopGame">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/GameWork"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/VideoGame"/>
        <rdfs:comment>A game that is played using physical components, such as cards, paper, pieces...</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Universe -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Universe">
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A setting in which fictional stories can take place. It may be a wholy fictional universe/world/timeline, or simply a fictional depicture of the real world with some distinct features common among its works.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/VideoGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/VideoGame">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/GameWork"/>
        <rdfs:comment>A game that runs on a computer or a computer-like device.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/Work -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Work">
        <rdfs:comment>A piece of art or media created by humans for artistic and/or entertainment purposes.</rdfs:comment>
    </owl:Class>
    


