<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/storyMedia/"
     xml:base="http://www.semanticweb.org/ontologies/storyMedia/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:storyMedia="http://www.semanticweb.org/ontologies/storyMedia#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/storyMedia"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#featuredIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#featuredIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#featuresSong"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#featuresSong -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#featuresSong">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/storyMedia/SingleWork"/>
                    <owl:Class>
                        <owl:complementOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/LiteraryWork"/>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>A song featured in a multimedial work. It doesn&apos;t matter if it was composed/recorded specifically for that work or not.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#follows -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#follows">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#follows"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasAlternativeVersion -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasAlternativeVersion">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isAlternativeVersionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasCover -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasCover">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isCoverOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>Cover version of a song performed by another music artist.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasDirectSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasDirectSequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDirectSequelOf"/>
        <rdfs:comment>A sequel that majorly features or references characters and plot points from its precedessing work. The sequel is harder to consume as a stand-alone work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasLooseSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasLooseSequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isLooseSequelOf"/>
        <rdfs:comment>A sequel that barely features or references characters and plot points from its precedessing work. It should be easy to consume as a stand-alone work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasMusicVideo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasMusicVideo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#RecordedSong"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#MusicVideo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasNewRecording -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasNewRecording">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isNewRecordingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#Song"/>
        <rdfs:comment>Re-recording of a song by the same music artist.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#hasSatire -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#hasSatire">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSatireOf"/>
        <rdfs:comment>A work that satirises or parodises this one.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#inspiredBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#inspiredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasInspiredWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isAdaptationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isAdaptationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isNewContinuityDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isAlternativeVersionOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isAlternativeVersionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isNewContinuityDerivedWorkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isContinuationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isContinuationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSequelOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasContinuation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isCoverOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isCoverOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isDirectAdaptationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isDirectAdaptationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isAdaptationOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDirectAdaptation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isDirectSequelOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isDirectSequelOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSequelOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isFollowedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isFollowedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A work that takes place after this one in some ordered sequence (e.g. chapters of a book, seasons of a show, songs on an album...).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isLooseAdaptationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isLooseAdaptationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isAdaptationOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasLooseAdaptation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isLooseSequelOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isLooseSequelOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSequelOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isNewContinuityDerivedWorkOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isNewContinuityDerivedWorkOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasRemake"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isNewRecordingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isNewRecordingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isPrequelOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isPrequelOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSameContinuityDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasPrequel"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isRebootOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isRebootOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isNewContinuityDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasReboot"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isRemakeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isRemakeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isNewContinuityDerivedWorkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isSameContinuityDerivedWorkOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isSameContinuityDerivedWorkOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isSatireOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isSatireOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isDerivedWorkOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isSequelOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isSequelOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSameContinuityDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isSideStoryOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isSideStoryOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSameContinuityDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSideStory"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#isSpinOffOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#isSpinOffOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia#isSameContinuityDerivedWorkOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSpinOff"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia#startsWithWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia#startsWithWork">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/storyMedia/Universe"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/belongsToFranchise -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/belongsToFranchise">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Franchise"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/belongsToSeries -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/belongsToSeries">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Series"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
        <rdfs:comment>Re-creation of an existing work&apos;s story in a different medium.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasContinuation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasContinuation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSequel"/>
        <rdfs:comment>A sequel that continues the same story, i.e. a single story that was split into multiple works.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A work that borrows some aspects from another work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasDirectAdaptation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasDirectAdaptation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation"/>
        <rdfs:comment>An adaptation that aims to retell the original story, despite possibly making some minor changes.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasInspiredWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasInspiredWork">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:comment>A work that borrows some minimal story or production aspects from another work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasLooseAdaptation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasLooseAdaptation">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasAdaptation"/>
        <rdfs:comment>An adaptation that borrows some plot points, character, themes, etc... from the original work, but introduces major changes to tell a new story.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <rdfs:comment>A story-based derivation that creates a new stand-alone continuity of the story.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasPrequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasPrequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
        <rdfs:comment>A work whose story is chronologically set before this one, but it was made later with the aim to explain previous plot points.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasReboot -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasReboot">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/CompoundWork"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/Work"/>
        <rdfs:comment>A work that aims to restart an existing series/franchise/etc...</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasRemake -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasRemake">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasNewContinuityDerivedWork"/>
        <rdfs:comment>Re-creation of an existing work&apos;s story that stays in the same medium.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasDerivedWork"/>
        <rdfs:comment>A story-based derivation that stays in the same continuity of the story.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSequel -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSequel">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/storyMedia/hasSameContinuityDerivedWork"/>
        <rdfs:comment>A work that was made after this one and its plot follows this one&apos;s.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/storyMedia/hasSideStory -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/storyMedia/hasSideStory">
